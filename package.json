{
  "name": "evil-eval",
  "version": "0.0.1",
  "description": "A JavaScript interpreter written in JavaScript.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "cover": "jest --coverage",
    "build": "tsc --target es2015 --declaration --sourceMap",
    "build-bundle": "webpack && webpack --env.es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkeylu/evil-eval.git"
  },
  "author": "jKey Lu <jkeylu@gmail.com> (https://github.com/jkeylu)",
  "license": "MIT",
  "dependencies": {
    "acorn": "^8.8.0",
    "acorn-walk": "^8.2.0"
  },
  "devDependencies": {
    "@types/estree": "0.0.38",
    "@types/jest": "^22.2.3",
    "@types/node": "^18.6.3",
    "jest": "^28.1.3",
    "string-replace-loader": "^2.1.1",
    "ts-jest": "^28.0.7",
    "ts-loader": "^4.2.0",
    "typescript": "^4.7.4",
    "webpack": "^4.5.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  }
}
